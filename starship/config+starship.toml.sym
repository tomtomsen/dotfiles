"$schema" = 'https://starship.rs/config-schema.json'

# Inserts a blank line between shell prompts
add_newline = true

[character]
success_symbol = '[](green)'
error_symbol = '[](red)'

[[battery.display]]
threshold = 10
style = 'red'
discharging_symbol = ' '

[[battery.display]]
threshold = 30
style = 'yellow'
discharging_symbol = ' '

[[battery.display]]
threshold = 100
style = 'green'
